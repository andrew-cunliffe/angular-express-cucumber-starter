{
    "name": "angular-express-cucumber",
    "version": "2.0.0",
    "author": "Andrew Cunliffe <andrew@sysen.co.uk>",
    "license": "MIT",
    "scripts": {
        "start": "node dist/server/main.js",
        "build": "npm run lint && npm run build:client && npm run build:server",
        "build:client": "ng build --prod --output-path=dist/client",
        "build:server": "tsc -p ./server",
        "develop": "npm run build:server && concurrently -k -s first \"npm run watch:server\" \"npm run watch:client\"",
        "lint": "tslint --force --project client/tsconfig.json && tslint --force --project server/tsconfig.json",
        "test": "npm run test:server && concurrently -k -s first \"npm start\" \"npm run test:client\"",
        "test:client": "protractor ./client/test/protractor.conf.js",
        "test:server": "./node_modules/.bin/istanbul cover ./node_modules/.bin/cucumber-js -- ./server/test/features --require ./server/test --exit",
        "watch:client": "ng build -w --output-path=dist/client",
        "watch:server": "concurrently -k -s first \"tsc -w -p ./server\" \"nodemon dist/server/main.js\"",
        "postinstall": "./node_modules/.bin/webdriver-manager update --versions.chrome 2.43 --versions.standalone 3.14.0"
    },
    "dependencies": {
        "@angular/animations": "7.0.1",
        "@angular/cdk": "7.0.1",
        "@angular/common": "7.0.1",
        "@angular/compiler": "7.0.1",
        "@angular/core": "7.0.1",
        "@angular/flex-layout": "7.0.0-beta.19",
        "@angular/forms": "7.0.1",
        "@angular/http": "7.0.1",
        "@angular/material": "7.0.1",
        "@angular/platform-browser": "7.0.1",
        "@angular/platform-browser-dynamic": "7.0.1",
        "@angular/router": "7.0.1",
        "classlist.js": "1.1.20150312",
        "core-js": "2.5.7",
        "express": "4.16.4",
        "hammerjs": "2.0.8",
        "rxjs": "6.3.3",
        "web-animations-js": "2.3.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.10.3",
        "@angular/cli": "7.0.3",
        "@angular/compiler-cli": "7.0.1",
        "@angular/language-service": "7.0.1",
        "@types/express": "4.16.0",
        "@types/node": "10.12.0",
        "chai": "4.2.0",
        "codelyzer": "4.5.0",
        "concurrently": "4.0.1",
        "cucumber": "5.0.2",
        "istanbul": "0.4.5",
        "nock": "10.0.1",
        "nodemon": "1.18.4",
        "protractor": "5.4.1",
        "protractor-cucumber-framework": "6.1.1",
        "shelljs": "0.8.2",
        "supertest": "3.3.0",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "typescript": "3.1.3"
    }
}
